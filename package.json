{
  "name": "graphql-api",
  "version": "1.0.0",
  "description": "movie api with graphql",
  "main": "index.js",
  "repository": "https://github.com/dev-hannahk/graphql-api.git",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.3",
    "graphql-yoga": "^1.18.3",
    "node-fetch": "2.1.2"
  },
  "scripts": {
